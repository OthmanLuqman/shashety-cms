{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false,
      "minLength": 3
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "like": {
      "type": "boolean",
      "default": false
    },
    "cover": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "trailerUrl": {
      "type": "string",
      "required": true
    }
  }
}
